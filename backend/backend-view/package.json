{
  "name": "backend",
  "version": "0.0.1",
  "scripts": {
    "clean": "npm cache clean -f",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "inspect": "vue-cli-service inspect",
    "update-dadi": "npm update",
    "update-xiqiao": "rm -rf package-lock.json && npm update uecomponents --registry http://npm.xiqiao.io && npm update ctviews --registry http://npm.xiqiao.io",
    "lint": "vue-cli-service lint --fix"
  },
  "main": "src/main.js",
  "files": [
    "src"
  ],
  "dependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-standard": "^5.1.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "deep-extend": "^0.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-vue-libs": "^4.0.0",
    "is-function": "^1.0.1",
    "less": "2.7.3",
    "less-loader": "5.0.0",
    "moment": "^2.29.2",
    "postcss": "^7.0.27",
    "postcss-loader": "^3.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ueboot": "^2.1.4",
    "view-design": "^4.1.3",
    "vue": "^2.6.11",
    "vuex": "^3.1.3",
    "vue-router": "latest"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/html-end-tags": "error",
      "vue/eqeqeq": "1",
      "semi": [
        1,
        "always"
      ],
      "no-console": "off",
      "no-var": 0,
      "no-eq-null": 1,
      "no-unused-vars": 1,
      "no-multi-spaces": 1,
      "vue/no-unused-vars": 1,
      "vue/no-use-v-if-with-v-for": 1,
      "vue/no-unused-components": 1,
      "vue/valid-v-for": 1,
      "no-undef": 1,
      "no-multiple-empty-lines": [
        1,
        {
          "max": 2
        }
      ],
      "comma-style": [
        1,
        "last"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >=9"
  ],
  "devDependencies": {
    "vue-template-compiler": "^2.6.11"
  }
}
